from selenium import webdriver
browser = webdriver.Safari(executable_path='/usr/bin/safaridriver')

browser.get('https://www.cnbc.com/economy/')
lst = browser.find_elements_by_class_name('Card-titleContainer')
lst2 = browser.find_elements_by_class_name('Card-time')
with open('cnbc title and time.txt', 'a') as f:
    f.write('cnbc economy\n')
    for i in range(len(lst)):
        f.write(lst2[i].text + ',' + lst[i].text + '\n')

browser.get('https://www.cnbc.com/finance/')
lst = browser.find_elements_by_class_name('Card-titleContainer')
lst2 = browser.find_elements_by_class_name('Card-time')
with open('cnbc title and time.txt', 'a') as f:
    f.write('\ncnbc finance\n')
    for i in range(len(lst)):
        f.write(lst2[i].text + ',' + lst[i].text + '\n')

browser.get('https://www.cnbc.com/search/?query=Amazon&qsearchterm=Amazon')
lst = browser.find_elements_by_class_name('SearchResult-searchResultTitle')
lst2 = browser.find_elements_by_class_name('SearchResult-publishedDate')

with open('cnbc title and time.txt', 'a') as f:
    f.write('\ncnbc Amazon\n')
    for i in range(len(lst2)):
        f.write(lst2[i].text + ',' + lst[i].text + '\n')

browser.quit()
